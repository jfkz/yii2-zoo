{
    "name": "worstinme/yii2-zoo",
    "description": "Yii content construction kit",
    "type": "yii2-extension",
    "keywords": ["yii2"," content"," CCK"," zoo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeny Zakirov",
            "email": "box@flyleaf.su"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-imagine": "*",
        "mihaildev/yii2-elfinder" : "dev-master",
        "bower-asset/select2": "4.*"
    },
    "autoload": {
        "psr-4": {
            "worstinme\\zoo\\": ""
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mihaildev/yii2-elfinder",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/worstinme/yii2-elfinder.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": [""]
                }
            }
        }
    ]
}
